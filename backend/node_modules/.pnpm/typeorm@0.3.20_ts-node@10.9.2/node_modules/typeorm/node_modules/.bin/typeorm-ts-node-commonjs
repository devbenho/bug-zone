#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mbanhawy/Desktop/hello-world/bugzone/backend/node_modules/.pnpm/typeorm@0.3.20_ts-node@10.9.2/node_modules/typeorm/node_modules:/Users/mbanhawy/Desktop/hello-world/bugzone/backend/node_modules/.pnpm/typeorm@0.3.20_ts-node@10.9.2/node_modules:/Users/mbanhawy/Desktop/hello-world/bugzone/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mbanhawy/Desktop/hello-world/bugzone/backend/node_modules/.pnpm/typeorm@0.3.20_ts-node@10.9.2/node_modules/typeorm/node_modules:/Users/mbanhawy/Desktop/hello-world/bugzone/backend/node_modules/.pnpm/typeorm@0.3.20_ts-node@10.9.2/node_modules:/Users/mbanhawy/Desktop/hello-world/bugzone/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-commonjs.js" "$@"
fi
